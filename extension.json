		"ext.blueSpiceExtendedSearch": {
			"scripts": [
				"ext.blueSpiceExtendedSearch.js",
				"ext.blueSpiceExtendedSearch.utils.js",
				"bs.extendedSearch/Lookup.js"
			],
			"dependencies": [
				"ext.bluespice",
				"mediawiki.api",
				"oojs",
				"oojs-ui"
			]
		},
